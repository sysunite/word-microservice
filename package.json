{
  "name": "word-microservice",
  "version": "1.0.0",
  "description": "Microservice for Word file manipulations using [docxtemplater](https://github.com/open-xml-templating/docxtemplater).",
  "main": "lib/index.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "./node_modules/coffee-script/bin/coffee src/index.coffee",
    "prepublish": "./node_modules/coffee-script/bin/coffee -o lib -c src && echo '#!/usr/bin/env node' | cat - ./lib/index.js > ./lib/index-exe.js && mv ./lib/index-exe.js ./lib/index.js",
    "test": "./node_modules/.bin/mocha && ./node_modules/.bin/istanbul report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysunite/word-microservice.git"
  },
  "author": "char",
  "bugs": {
    "url": "https://github.com/sysunite/word-microservice/issues"
  },
  "homepage": "https://github.com/sysunite/word-microservice#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "coffee-script": "^1.12.4",
    "config": "^1.25.1",
    "cuid": "^1.3.8",
    "express": "^4.14.1",
    "moment": "^2.17.1",
    "multer": "^1.3.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.12.4",
    "istanbul": "^0.4.3",
    "mocha": "^2.2.5",
    "should": "^11.2.0",
    "sinon": "^1.15.4",
    "supertest": "^3.0.0"
  }
}
